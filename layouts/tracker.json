{
	"tracker_capture_item" : {
		"type" : "container",
		"content" : {
			"type" : "itemgrid",
			"h_alignment" : "center",
			"item_margin" : "1,2",
			"rows" : [
				[ "earthorb", "fireorb", "waterorb", "airorb" ],
				[ "lute", "key", "rod", "oxyale", "chime", "cube" ],
				[ "bridge", "ship", "canal", "canoe", "floater" ],
				[ "adamant", "tnt", "crown", "crystal", "herb" ],
				[ "garland", "ruby", "tail", "slab", "bottle" ]
			]
		}
	},
	"tracker_default" : {
		"type" : "container",
		"background" : "#5f6393",
		"content" : {
			"type" : "dock",
			"dropshadow" : false,
			"content" : [
				{
					"type" : "dock",
					"dock" : "left",
					"v_alignment" : "stretch",
					"content" : [
						{
							"type" : "group",
							"header" : "Items",
							"dock" : "top",
							"content" : {
								"type" : "layout",
								"key" : "shared_item_grid"
							}
						},
						{
							"type" : "group",
							"header" : "Locations",
							"dock" : "top",
							"content" : {
								"type" : "layout",
								"key" : "shared_locations_grid"
							}
						},
						{
							"type" : "group",
							"header" : "Flags",
							"dock" : "top",
							"content" : {
								"type" : "layout",
								"key" : "shared_flags_grid"
							}
						},
						{
							"type" : "group",
							"header" : "Pinned Locations",
							"content" : {
								"type" : "recentpins",
								"style" : "wrap",
								"h_alignment" : "stretch",
								"v_alignment" : "stretch",
								"orientation" : "vertical",
								"compact" : true
							}
						}
					]
				},
				{
					"type" : "tabbed",
					"tabs" : [
						{
							"title" : "Incentive Locations",
							"content" : {
								"type" : "map",
								"maps" : [ "incentives" ]
							}
						},
						{
							"title" : "Overworld",
							"content" : {
								"type" : "map",
								"maps" : [ "overworld" ]
							}
						}
					]
				}
			]
		}
	},
	"tracker_horizontal" : {
		"type" : "container",
		"background" : "#5f6393",
		"content" : {
			"type" : "dock",
			"dropshadow" : false,
			"content" : [
				{
					"type" : "dock",
					"dock" : "bottom",
					"content" : [
						{
							"type" : "group",
							"header" : "Items",
							"dock" : "left",
							"margin" : "0,0,3,0",
							"content" : {
								"type" : "layout",
								"h_alignment" : "center",
								"v_alignment" : "center",
								"key" : "shared_item_grid"
							}
						},
						{
							"type" : "group",
							"header" : "Locations",
							"dock" : "left",
							"margin" : "0,0,3,0",
							"content" : {
								"type" : "layout",
								"h_alignment" : "center",
								"v_alignment" : "center",
								"key" : "shared_locations_grid"
							}
						},
						{
							"type" : "group",
							"header" : "Flags",
							"dock" : "left",
							"margin" : "0,0,3,0",
							"content" : {
								"type" : "layout",
								"h_alignment" : "center",
								"v_alignment" : "center",
								"key" : "shared_flags_grid"
							}
						},
						{
							"type" : "group",
							"header" : "Pinned Locations",
							"content" : {
								"type" : "recentpins",
								"style" : "wrap",
								"h_alignment" : "stretch",
								"v_alignment" : "stretch",
								"orientation" : "horizontal",
								"compact" : true
							}
						}
					]
				},
				{
					"type" : "tabbed",
					"tabs" : [
						{
							"title" : "Incentive Locations",
							"content" : {
								"type" : "map",
								"maps" : [ "incentives" ]
							}
						},
						{
							"title" : "Overworld",
							"content" : {
								"type" : "map",
								"maps" : [ "overworld" ]
							}
						}
					]
				}
			]
		}
	}
}
